namespace tower.world.data;

enum TileType : uint8 {
    NONE,
    TERRAIN,
}

enum TileState : uint8 {
    NONE = 0,
    BLOCKED = 1,
}

enum ObjectType : uint16 {
    NONE,
}

struct Vector2 {
    x: float32;
    y: float32;
}

struct Vector2i {
    x: int32;
    y: int32;
}

struct TileData {
    type: TileType;
    state: TileState;
}
